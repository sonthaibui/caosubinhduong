<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!--inherits the default sales report  and override its name  and file-->
    <record id="paperformat_stockpicking" model="report.paperformat">
        <field name="name">Format Report Batch Picking</field>
        <field name="default" eval="True"/>
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">35</field>
        <field name="margin_bottom">17</field>
        <field name="margin_left">10</field>
        <field name="margin_right">10</field>
        <field name="header_spacing">25</field>
        <field name="dpi">90</field>
    </record>

    <record id="paperformat_stockpicking_receipts" model="report.paperformat">
        <field name="name">Format Report Stock Picking Receipts</field>
        <field name="default" eval="True"/>
        <field name="format">A5</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">35</field>
        <field name="margin_bottom">17</field>
        <field name="margin_left">10</field>
        <field name="margin_right">10</field>
        <field name="header_spacing">25</field>
        <field name="dpi">90</field>
    </record>
    
</odoo>