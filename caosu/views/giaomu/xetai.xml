        <field name="type">ir.actions.act_window</field>
        <field name="res_model">rubber.sell</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'group_by': ['ngay:year', 'ngay:month']}</field>
    </record>

    
</odoo>
