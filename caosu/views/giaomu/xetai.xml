    <record id="action_set_sanpham_dong_global" model="ir.actions.server">
        <field name="name">Set Filter: Mũ đông</field>
        <field name="model_id" ref="model_company_truck"/>
        <field name="binding_model_id" ref="model_company_truck"/>
