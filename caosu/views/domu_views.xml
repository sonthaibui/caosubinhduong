<odoo>
  <record id="view_do_mu_tree" model="ir.ui.view">
    <field name="model">do.mu</field>
    <field name="arch" type="xml">
      <tree>
        <field name="ngay_hieuluc"/>
        <field name="to"/>
        <field name="do_mutap"/>
        <field name="do_muday"/>
        <field name="do_mudong"/>
        <field name="do_muchen"/>
        <field name="ghi_chu"/>
      </tree>
    </field>
  </record>

  <record id="view_do_mu_form" model="ir.ui.view">
    <field name="model">do.mu</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="ngay_hieuluc"/>
          <field name="to"/>
          <field name="do_mutap"/>
          <field name="do_muday"/>
          <field name="do_mudong"/>
          <field name="do_muchen"/>
          <field name="ghi_chu"/>
        </group>
      </form>
    </field>
  </record>

  <record id="action_do_mu" model="ir.actions.act_window">
    <field name="name">Độ mủ</field>
    <field name="res_model">do.mu</field>
    <field name="view_mode">tree,form</field>
  </record>  
</odoo>