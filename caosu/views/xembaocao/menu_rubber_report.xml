<odoo>
    <!-- Define an act_url action that calls the rubber report controller -->
    <record id="action_report_rubber" model="ir.actions.report">
            <field name="name">Rubber Report</field>
            <field name="model">rubber</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">caosu.rubber_report_template</field>
            <field name="attachment_use" eval="False"/>
        </record>   
   
    
    <menuitem id="menu_rubber_report" name="Mũ Công nhân" parent="menu_report_root"
              action="action_report_rubber" sequence="1"/>
</odoo>