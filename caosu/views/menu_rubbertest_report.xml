<?xml version="1.0" encoding="utf-8"?>
<odoo>
    

    <!-- Report action definition -->
    <record id="action_report_rubbertest" model="ir.actions.report">
        <field name="name">Rubber Test Report</field>
        <field name="model">rubber.test</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">caosu.rubbertest_report_template</field>
        <field name="report_file">caosu.rubbertest_report_template</field>
        <field name="binding_model_id" ref="model_rubber_test"/>
        <field name="binding_type">report</field>
    </record>

    <!-- Root menu for Reports -->
    <menuitem id="menu_report_root" 
              name="Xem báo cáo" 
              sequence="10"/>
    <!-- Report menu item -->
    <menuitem id="menu_rubbertest_html_report" 
              name="Mũ thí nghiệm" 
              parent="menu_report_root" 
              action="action_report_rubbertest" 
              sequence="2"/>

   
    
</odoo>