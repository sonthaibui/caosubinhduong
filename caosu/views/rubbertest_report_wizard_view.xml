<odoo>
    <record id="view_rubbertest_report_wizard_form" model="ir.ui.view">
        <field name="name">rubbertest.report.wizard.form</field>
        <field name="model">rubbertest.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Rubber Test Report Wizard">
                <group>
                    <field name="nhom"/>
                    <field name="to"/>
                    <field name="lo"/>
                    <field name="compare_field"/>
                    <field name="detail_field"/>
                    <field name="sort_order"/>
                    <field name="dao_kt_up"/>
                </group>
                <footer>
                    <button string="Print Report" type="object" name="print_report" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_rubbertest_report_wizard" model="ir.actions.act_window">
        <field name="name">Rubber Test Report Wizard</field>
        <field name="res_model">rubbertest.report.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>