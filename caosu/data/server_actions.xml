        <field name="code">action = env['rubber.date'].recompute_haohut_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
    <record id="action_recompute_all_ban" model="ir.actions.server">
        <field name="name">Recompute All Ban</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_ban_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
</odoo>