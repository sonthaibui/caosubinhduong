<odoo>
    <record id="action_recompute_nuoc_giao_all" model="ir.actions.server">
        <field name="name">Recompute All nuoc_giao</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_giao_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
    <record id="action_recompute_all_rubberdate" model="ir.actions.server">
