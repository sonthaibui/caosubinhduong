    
<odoo>
    <!-- Server Action to Recompute do_phancay for ALL Rubber records -->
    <record id="action_recompute_do_phancay_all" model="ir.actions.server">
        <field name="name">Recompute do_phancay ALL Records</field>
        <field name="model_id" ref="caosu.model_rubber"/>
        <field name="binding_model_id" ref="caosu.model_rubber"/>
        <field name="binding_view_types">list</field>
        <field name="binding_type">action</field>
        <field name="state">code</field>
        <field name="code">action = env['rubber'].recompute_do_phancay_all()</field>
    </record>
    
    <record id="action_recompute_nuoc_giao_all" model="ir.actions.server">
        <field name="name">Recompute All nuoc_giao</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_giao_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
    <record id="action_recompute_all_rubberdate" model="ir.actions.server">
        <field name="name">Recompute All Calculated Fields</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_all_records()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
    <record id="action_recompute_haohut_all" model="ir.actions.server">
        <field name="name">Recompute Hao Hut All</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_haohut_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>
    <record id="action_recompute_all_ban" model="ir.actions.server">
        <field name="name">Recompute All Ban</field>
        <field name="model_id" ref="caosu.model_rubber_date"/>
        <field name="state">code</field>
        <field name="code">action = env['rubber.date'].recompute_ban_all()</field>
        <field name="binding_model_id" ref="caosu.model_rubber_date"/>
        <field name="binding_type">action</field>
    </record>

    <!-- Server Action to Recompute Quy Kho for Rubber records -->
    <record id="action_recompute_quykho" model="ir.actions.server">
        <field name="name">Recompute Quy Kho</field>
        <field name="model_id" ref="caosu.model_rubber"/>
        <field name="binding_model_id" ref="caosu.model_rubber"/>
        <field name="binding_view_types">list,form</field>
        <field name="binding_type">action</field>
        <field name="state">code</field>
        <field name="code">action = records.recompute_quykho_selected()</field>
    </record>

    <!-- Server Action to Recompute Quy Kho for ALL Rubber records -->
    <record id="action_recompute_quykho_all" model="ir.actions.server">
        <field name="name">Recompute Quy Kho ALL Records</field>
        <field name="model_id" ref="caosu.model_rubber"/>
        <field name="binding_model_id" ref="caosu.model_rubber"/>
        <field name="binding_view_types">list</field>
        <field name="binding_type">action</field>
        <field name="state">code</field>
        <field name="code">action = env['rubber'].recompute_quykho_all()</field>
    </record>
</odoo>