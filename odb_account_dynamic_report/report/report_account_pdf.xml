<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="om_account_reports_pdf">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="oe_structure"/>

                        <t t-if="r_header">
                            <t t-raw="r_header"/>
                        </t>
                        <div class="row mt32">
                            <div t-raw="report_body" class="dynamic_body"/>
                        </div>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>
